<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:fitsSystemWindows="true"
    tools:context=".DataRegist">

    <LinearLayout
        android:id="@+id/MainLinear"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">

        <FrameLayout
            android:layout_width="fill_parent"
            android:id="@+id/frameLayout1"
            android:layout_height="fill_parent">




            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="vertical">





                <ImageView
                    android:id="@+id/Guidance2"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="16"
                    android:background="@drawable/lsl"
                    android:adjustViewBounds="true" >
                </ImageView>


                <LinearLayout
                    android:id="@+id/f"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="2"
                    android:background="@drawable/gura"
                    android:orientation="horizontal" >

                    <Button
                        android:id="@+id/back"
                        android:layout_width="0dip"
                        android:layout_height="fill_parent"
                        android:layout_weight="1.6"
                        android:background="@drawable/bodysize"
                        android:text=""
                        />

                    <LinearLayout
                        android:id="@+id/fu"
                        android:layout_width="0dip"
                        android:layout_weight="4"
                        android:layout_height="fill_parent"
                        android:orientation="horizontal" >



                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:orientation="vertical">



                            <RelativeLayout
                                android:id="@+id/btn3"
                                android:layout_width="fill_parent"
                                android:layout_height="0dp"
                                android:layout_weight="1">

                                <TextView
                                    android:id="@+id/textView2"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:text="ヒョウタンのような体型ですね。"
                                    android:textAppearance="?android:attr/textAppearanceLarge" />

                            </RelativeLayout>



                        </LinearLayout>


                    </LinearLayout>
                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="vertical">



                <LinearLayout
                    android:id="@+id/bloern"
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:layout_weight="2.0"
                    android:orientation="horizontal">




                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:animateLayoutChanges="true"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:animateLayoutChanges="true"
                        android:orientation="vertical" >

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="0dp"
                            android:layout_weight="0.8"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="0dip"
                                android:layout_weight="0.3"
                                android:layout_height="fill_parent"
                                android:background="#00000000"/>

                            <RelativeLayout
                                android:layout_width="0dip"
                                android:layout_weight="0.5"
                                android:layout_height="fill_parent">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textAppearance="?android:attr/textAppearanceLarge"
                                    android:textColor="@color/cri"
                                    android:textSize="35sp"
                                    android:text="W +10"
                                    android:paddingTop="20dp"
                                    android:id="@+id/setwaste"
                                    android:layout_centerHorizontal="true"

                                    android:layout_centerVertical="true"/>

                            </RelativeLayout>

                            <LinearLayout
                                android:layout_width="0dip"
                                android:layout_weight="2"
                                android:layout_height="fill_parent"
                                android:background="#00000000"/>
                        </LinearLayout>



                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:animateLayoutChanges="true"
                        android:orientation="vertical" >

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="0dp"
                            android:layout_weight="0.8"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="0dip"
                                android:layout_weight="0.21"
                                android:layout_height="fill_parent"
                                android:background="#00000000"/>

                            <RelativeLayout
                                android:layout_width="0dip"
                                android:layout_weight="0.5"
                                android:layout_height="fill_parent">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textAppearance="?android:attr/textAppearanceLarge"
                                    android:text="H -8"
                                    android:id="@+id/sethip"
                                    android:textSize="35sp"
                                    android:textColor="@color/dsb"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"/>
                            </RelativeLayout>

                            <LinearLayout
                                android:layout_width="0dip"
                                android:layout_weight="2"
                                android:layout_height="fill_parent"
                                android:background="#00000000"/>
                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:animateLayoutChanges="true"
                        android:orientation="vertical" >

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="0dp"
                            android:layout_weight="0.8"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="0dip"
                                android:layout_weight="0.23"
                                android:layout_height="fill_parent"
                                android:background="#00000000"/>

                            <RelativeLayout
                                android:layout_width="0dip"
                                android:layout_weight="0.5"
                                android:layout_height="fill_parent">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textAppearance="?android:attr/textAppearanceLarge"
                                    android:text="T +5"
                                    android:textColor="@color/cri"
                                    android:textSize="35sp"
                                    android:paddingBottom="25dp"
                                    android:id="@+id/setfutomomo"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"/>

                            </RelativeLayout>

                            <LinearLayout
                                android:layout_width="0dip"
                                android:layout_weight="2"
                                android:layout_height="fill_parent"
                                android:background="#00000000"/>
                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>




                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1.7"
                    android:orientation="vertical">


                    </LinearLayout>



                </LinearLayout>






        </FrameLayout>



    </LinearLayout>

</android.support.design.widget.CoordinatorLayout>
